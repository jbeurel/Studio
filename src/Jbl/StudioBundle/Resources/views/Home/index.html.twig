					<section id="reserver">
						<h2>Réserver</h2>
						<div class="row">
						
							<p>
								Vous pouvez nous contacter pour <b>réserver votre semaine de vacance</b> ou pour avoir plus d'informations.
							</p>
						
							<div class="span11">
							
							
							{%  if formIsSend %}
							<div class="alert-message success">
						    	<p>Votre message a bien été envoyé ! Nous vous répondrons au plus vite. Merci.</p>
						    </div>	
							{%  endif %}
							
							{% if form_errors(form) %}
							<div class="alert-message warning">
						    	<div>{{ form_errors(form) }}</div>
						    </div>
							{% endif %}
				
							<form action="#reserver" method="post" {{ form_enctype(form) }}>
	
								<div class="clearfix">
				
									{{ form_label(form.fromMail, 'Votre Email* :') }}
								    <div class="input">
								    	{{ form_widget(form.fromMail,{ 'attr': {'class': 'xlarge', 'size':'30'} }) }}
										<span class="help-inline">Champs obligatoire !</span>
									</div>
								</div>
								
								<div class="clearfix">
				
									{{ form_label(form.name, 'Votre Nom :') }}
								    <div class="input">
								    	{{ form_widget(form.name,{ 'attr': {'class': 'xlarge', 'size':'30'} }) }}
									</div>
								</div>
								
								<div class="clearfix">
				
									{{ form_label(form.phone, 'Votre Téléphone :') }}
								    <div class="input">
								    	{{ form_widget(form.phone,{ 'attr': {'class': 'xlarge', 'size':'30'} }) }}
									</div>
								</div>
								
								<div class="clearfix">
									{{ form_label(form.message, 'Votre Message* :') }}
								    <div class="input">
								    	{{ form_widget(form.message,{ 'attr': {'class': 'xlarge', 'rows':'3'} }) }}
								    	<span class="help-inline">Champs obligatoire !</span>
									</div>
								</div>
								
								{{ form_widget(form) }}
								
								
								<div class="actions">
							        <button class="btn success" type="submit">Envoyer le message</button>
							    </div>
							</form>

							</div>
						
							<div class="span5">
								{% if phoneAvailable %}
								<p style="text-align:center">
									Vous pouvez nous contacter par téléphone.
									<br/><br/><br/>
									<span class="btn disabled success large">02.02.02.02.02</span>
								</p>
